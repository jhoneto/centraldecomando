<ul class="tabs"> 
	<li><%= link_to "#{t("bootstrap-rails.list", :default => "List")}", layout_layout_fields_path  %></li> 
	<li><%= link_to "#{t("bootstrap-rails.new", :default => "New")}", new_layout_layout_field_path %></li>  
	<li class="active"><%= link_to "#{t("bootstrap-rails.edit", :default => "Edit")}", edit_layout_layout_field_path %></li>
</ul>
<h2 class="title">Editar Campo</h2>                            

<%= form_for @layout_field, :url => layout_layout_field_path(@layout_field), :html => { :class => "form-stacked" } do |f| -%>
