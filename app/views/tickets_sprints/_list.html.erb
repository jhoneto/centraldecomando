      	<table class="table">
	        <tr>             
	          <th class="first">ID Ticket</th>
	          <th>Título</th>
	          <th>Status</th>
	          <th>Estimativa</th>
	          <th>Responsável</th>
	          <th class="last">&nbsp;</th>
	        </tr>
	        <% for ts in @tickets_sprints -%>
	        <tr class="<%= cycle("odd", "even") %>">
	          <td><%= ts.ticket_id %></td>
	          <td><%= ts.ticket.title %></td>
	          <td><%= ts.ticket.ticket_status.name%></td> 
	          <td><%= ts.estimate%></td>
		      <td><%= ts.ticket.user_owner.name%></td>                    
	          <td class="last">
                <%= link_to "#{t("bootstrap-rails.edit", :default => "Edit")}", edit_sprint_ticket_sprint_path(@sprint, ts), :remote => true %> |
                <%= link_to "#{t("bootstrap-rails.delete", :default => "Delete")}", sprint_ticket_sprint_path(@sprint, ts), :remote => true, :method => :delete, :confirm => "#{t("bootstrap-rails.confirm", :default => "Are you sure?")}" %> 
	          </td>
	        </tr>          
	        <% end -%>
	      </table>
	      