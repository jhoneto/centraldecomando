<ul class="tabs"> 
	<li><%= link_to "#{t("bootstrap-rails.list", :default => "List")}", sprints_path  %></li> 
	<li><%= link_to "#{t("bootstrap-rails.new", :default => "New")}", new_sprint_path %></li>  
	<li class="active"><%= link_to "#{t("bootstrap-rails.edit", :default => "Edit")}", edit_sprint_path %></li>
</ul>
<h2 class="title"><%= t("bootstrap-rails.edit", :default => "Edit") %> Sprint</h2>                            

<%= form_for @sprint, :url => sprint_path(@sprint), :html => { :class => "form-stacked" } do |f| -%>
    <%= render :partial => "form", :locals => {:f => f} %>
<% end -%>                        

<% content_for :sidebar, render(:partial => 'sidebar') -%> 

