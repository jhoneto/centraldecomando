<ul class="tabs"> 
	<li><%= link_to "#{t("bootstrap-rails.list", :default => "List")}", sprints_path  %></li> 
	<li><%= link_to "#{t("bootstrap-rails.new", :default => "New")}", new_sprint_path %></li>  
	<li class="active"><%= link_to "#{t("bootstrap-rails.show", :default => "Show")}", sprint_path %></li>
</ul>    


    <p>
      <b><%= t("activerecord.attributes.sprint.project_id", :default =>"Project") %>:</b>
      <%= @sprint.project_id %>
    </p>

    <p>
      <b><%= t("activerecord.attributes.sprint.sequence", :default =>"Sequence") %>:</b>
      <%= @sprint.sequence %>
    </p>

    <p>
      <b><%= t("activerecord.attributes.sprint.date_of_beginning", :default =>"Date of beginning") %>:</b>
      <%= @sprint.date_of_beginning %>
    </p>

    <p>
      <b><%= t("activerecord.attributes.sprint.date_of_end", :default =>"Date of end") %>:</b>
      <%= @sprint.date_of_end %>
    </p>

    <p>
      <b><%= t("activerecord.attributes.sprint.status", :default =>"Status") %>:</b>
      <%= @sprint.status %>
    </p>

    <p>
      <b><%= t("activerecord.attributes.sprint.capacity", :default =>"Capacity") %>:</b>
      <%= @sprint.capacity %>
    </p>
                  

<div class="actions">
	<%= link_to t("ootstrap-rails.edit", :default=> "Edit"), edit_sprint_path(@sprint), :class => "btn large primary" %>
    <%= link_to t("ootstrap-rails.delete", :default => "Delete"), sprint_path(@sprint), :method => "delete", :class => "btn large", :confirm => "#{t("web-app-theme.confirm", :default => "Are you sure?")}" %>
</div>      
    
<% content_for :sidebar, render(:partial => 'sidebar') -%>

