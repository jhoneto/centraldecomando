<ul class="tabs"> 
	<li><%= link_to "#{t("bootstrap-rails.list", :default => "List")}", filters_path  %></li>  
	<li><%= link_to "#{t("bootstrap-rails.new", :default => "New")}", new_filter_path %></li> 
	<li class="active"><%= link_to 'Filtro executado', '#'%></li>
</ul>   
<h2 class="title"><%= @filter.name  %>
<table class="bordered-table zebra-striped">
    <thead>  
	  <% @columns.each do |col| %>
	    <th><%= col %></th>
	  <% end %>           
    </thead>                         
    <tbody>
      	<%  @result.each do |f| -%>
        <tr class="<%= cycle("odd", "even") %>">
          <% @columns.each do |col| %>
		    <td><%= f[col] %></td>
		  <% end %>
        </tr>          
        <% end -%>
    </tbody>
</table>
<%= will_paginate @result  %>
